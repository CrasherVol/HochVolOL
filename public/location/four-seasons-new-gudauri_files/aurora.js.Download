jQuery(function() {
	search = jQuery('.search');
	modlang = jQuery('.modlang');
	currency = jQuery('.currency');
	modatlogin = jQuery('.modlogin');
	ha = jQuery('.hidden-area');
    bookmarks = jQuery('#aubookmarks');
    pmessages = jQuery('#aumessages');

    jQuery(document).click(function(){ search.fadeOut(); modlang.slideUp(); currency.slideUp(); modatlogin.slideUp(); bookmarks.slideUp(); pmessages.slideUp(); });

    search.click(function(e){ e.stopPropagation(); });
    modlang.click(function(e){ e.stopPropagation(); });
    currency.click(function(e){ e.stopPropagation(); });
    modatlogin.click(function(e){ e.stopPropagation(); });
	ha.click(function(e){ e.stopPropagation(); });
    bookmarks.click(function(e){ e.stopPropagation(); });
    pmessages.click(function(e){ e.stopPropagation(); });

	jQuery('.asearch').click(function(e){
        if (search.is(':visible') === false) {
        	e.stopPropagation();
            search.fadeIn();
            modlang.slideUp();
            currency.slideUp();
            modatlogin.slideUp();
            bookmarks.slideUp();
            pmessages.slideUp();
        }
    });

    jQuery('.alang').click(function(e){
        if (modlang.is(':visible') === false) {
        	e.stopPropagation();
            modlang.slideDown();
            search.fadeOut();
            currency.slideUp();
            modatlogin.slideUp();
            bookmarks.slideUp();
            pmessages.slideUp();
        }
    });

    jQuery('.acur').click(function(e){
        if (currency.is(':visible') === false) {
        	e.stopPropagation();
            currency.slideDown();
            search.fadeOut();
            modlang.slideUp();
            modatlogin.slideUp();
            bookmarks.slideUp();
            pmessages.slideUp();
        }
    });

    jQuery('.alogin').click(function(e){
        if (modatlogin.is(':visible') === false) {
        	e.stopPropagation();
            modatlogin.slideDown();
            search.fadeOut();
            modlang.slideUp();
            currency.slideUp();
            bookmarks.slideUp();
            pmessages.slideUp();
        }
    });

    jQuery('.abookm').click(function(e){
        if (bookmarks.is(':visible') === false) {
            e.stopPropagation();
            modatlogin.slideUp();
            search.fadeOut();
            modlang.slideUp();
            currency.slideUp();
            bookmarks.slideDown();
            pmessages.slideUp();
        }
    });

    jQuery('.aipm').click(function(e){
        if (pmessages.is(':visible') === false) {
            e.stopPropagation();
            modatlogin.slideUp();
            search.fadeOut();
            modlang.slideUp();
            currency.slideUp();
            bookmarks.slideUp();
            pmessages.slideDown();
        }
    });
});